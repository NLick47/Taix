<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="UI.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <SimpleTheme />
	</Application.Styles>
	
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Resources/Themes/Dark.axaml"/>
				<!--<ResourceInclude Source="/Resources/Themes/Light.axaml"/>-->
				<ResourceInclude Source="/Resources/Localization/StringResources.en.axaml"/>
				<ResourceInclude Source="/Themes/Base/Icon.axaml"/>
				
				<ResourceInclude Source="/Themes/Window/DefaultWIndow.axaml"/>
				<ResourceInclude Source="/Themes/Window/HideWindow.axaml"/>
				
				<ResourceInclude Source="/Themes/Navigation/Navigation.axaml"/>
				<ResourceInclude Source="/Themes/Navigation/NavigationItem.axaml"/>
				<ResourceInclude Source="/Themes/Button/IconButton.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>	
	</Application.Resources>

	<!--<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.ThemeDictionaries>

				
			</ResourceDictionary.ThemeDictionaries>
		</ResourceDictionary>
	</Application.Resources>-->
</Application>