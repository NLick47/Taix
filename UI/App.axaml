<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="UI.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <SimpleTheme />
		<StyleInclude Source="/Themes/Base/Icon.axaml"/>
		<StyleInclude Source="/Themes/Window/DefaultWindow.axaml"/>
		<StyleInclude Source="/Themes/Button/IconButton.axaml"/>
    </Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Resources/Themes/Dark.axaml"/>
				<ResourceInclude Source="/Resources/Themes/Light.axaml"/>
				<ResourceInclude Source="/Resources/Localization/StringResources.en.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>