<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	
             x:Class="UI.App">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
		<SimpleTheme/>
		<StyleInclude Source="avares://Avalonia.Controls.ColorPicker/Themes/Simple/Simple.xaml" />
	</Application.Styles>
             
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Resources/Themes/Common.axaml"/>
				<ResourceInclude Source="/Resources/Themes/Theme.axaml"/>
				<ResourceInclude Source="/Themes/Generic.axaml"/>
				<!--<ResourceInclude Source="/Resources/Localization/StringResources.en.axaml"/>-->
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>	
	</Application.Resources>

</Application>